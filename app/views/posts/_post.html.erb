<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>

  <p>
    <strong>Tag:</strong>
    <%= post.tag %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= post.image_url %>
  </p>

</div>
